#pragma once


struct ProcessCounter_M
{

	void init(){
		mActiveCount++;
	}

    bool isExeReady() const {return true;}

	bool isDelReady() const {return true;}

	void makePreExe(){}

	void makePreDel(){
		mActiveCount--;	
	}

	void makePostExe(){}

	size_t getCount(){
		return mActiveCount;
	}

private:

	static size_t mActiveCount;
};

size_t Counter_M::mActiveCount = 0;
